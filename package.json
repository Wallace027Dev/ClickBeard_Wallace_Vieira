{
  "name": "barbershop-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:db:start": "docker-compose up -d",
    "dev:db:stop": "docker-compose down",
    "dev:db:restart": "npm run dev:db:stop && npm run dev:db:start",
    "dev:server:start": "nodemon src/server.ts",
    "dev:server:stop": "killall node",
    "dev:server:restart": "npm run dev:server:stop && npm run dev:server:start",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force"
  },
