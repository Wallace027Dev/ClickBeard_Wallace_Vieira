{
  "name": "barbershop-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:restart": "npm run dev:db:stop && npm run dev:db:start",
    "server:start": "nodemon src/server.ts",
    "server:stop": "killall node",
    "server:restart": "npm run dev:server:stop && npm run dev:server:start",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:start": "npm run prisma:migrate && npm run prisma:seed",
    "build": "npm run prisma:start && tsc",
    "start": "node dist/src/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "5.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.1",
    "multer": "^2.0.2",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@prisma/client": "^6.12.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.6.2",
    "@types/multer": "^2.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2"
  }
}
